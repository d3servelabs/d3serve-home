    "tailwind-merge": "2.6.0",
    "tailwind-scrollbar": "3.1.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-3d": "1.0.7",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-radix": "3.0.5",
    "tsx": "4.19.2",
    "eslint": "9.17.0",
    "eslint-config-next": "15.1.3",
