{
  "name": "d3serve-home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern --write ./ src",
    "type": "tsc --noEmit --incremental",
    "format": "pnpm run prettier && pnpm run lint && pnpm run type",
    "bootstrap": "pnpm install && pnpm run format",
    "postbuild": "next-sitemap",
    "sitemap": "next-sitemap",
    "clean": "find . -type d \\( -name 'node_modules' -o -name '.next' -o -name '.turbo' \\) -prune -exec rm -rf {} +",
    "svgr": "svgr -d src/components/icons --expand-props='end' --no-svgo --typescript src/icons --no-prettier && prettier src/components/icons --write",
    "scripts:checks": "tsx src/scripts/checks.ts",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "framer-motion": "11.15.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "web-vitals": "4.2.4",
    "@amplitude/analytics-types": "2.8.4",
    "nextjs-google-analytics": "2.3.7",
    "@amplitude/analytics-browser": "2.11.11",
    "@amplitude/experiment-js-client": "1.14.1",
    "next": "15.1.3",
    "react-responsive": "10.0.0",
    "next-nprogress-bar": "2.4.3",
    "next-themes": "0.4.4",
    "react-combine-providers": "0.9.6",
    "dotenv": "16.4.7",
    "@radix-ui/react-tabs": "1.1.2",
    "autoprefixer": "10.4.20",
    "@hookform/resolvers": "3.9.1",
    "@t3-oss/env-core": "0.11.1",
    "@t3-oss/env-nextjs": "0.11.1",
    "styled-components": "6.1.13",
    "tailwind-merge": "2.6.0",
    "clsx": "2.1.1",
    "lucide-react": "0.469.0",
    "react-hook-form": "7.54.2",
    "class-variance-authority": "0.7.1",
    "uuid": "11.0.3",
    "zod": "3.24.1",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.1.3",
    "@svgr/cli": "8.1.0",
    "@types/jest": "29.5.14",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@svgr/webpack": "8.1.0",
    "typescript": "5.7.2",
    "@types/node": "22.10.5",
    "@types/react": "19.0.2",
    "@tailwindcss/forms": "0.5.9",
    "@chromatic-com/storybook": "3.2.3",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-onboarding": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@tailwindcss/typography": "0.5.15",
    "eslint-plugin-storybook": "0.11.2",
    "eslint-plugin-tailwindcss": "3.17.5",
    "@types/react-dom": "19.0.2",
    "postcss": "8.4.49",
    "next-sitemap": "4.2.3",
    "prettier": "3.4.2",
    "@storybook/nextjs": "8.4.7",
